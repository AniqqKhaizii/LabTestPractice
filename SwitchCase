//Switch Case
#include<iostream>
using namespace std;

int main()
{
	char grade;
	cout << "Please enter your midterm grade" << endl;
	cin >> grade;

	switch (grade)
	{
	case 'A':
		cout << "Excellent" << endl;
		break;
	case 'B':
		cout << "Very Good" << endl;
		break;
	case 'C':
		cout << "Passed" << endl;
		break;
	case 'D':
	case 'F':
		cout << "Not good. Study harder!" << endl;
		break;
	default:
		cout << "Not Possible Grade" << endl;
	}
	cout << "\nEnd of program." << endl;
	return 0;
}
